<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImagesSelectView">
        <attr name="optionalMax" format="integer" />
        <attr name="multiSelect" format="boolean" />
        <attr name="isDigit" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GalleryToolbar">
        <attr name="type">
            <enum name="seleted" value="1" />
            <enum name="preview" value="2" />
            <enum name="edit" value="3" />
        </attr>
    </declare-styleable>
</resources>